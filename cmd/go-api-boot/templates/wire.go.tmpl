// +build wireinject
// The build tag makes sure the stub is not built in the final build.

package main

import (
    "github.com/google/wire"
    "github.com/SaiNageswarS/go-api-boot/cloud"
)

type App struct {
    Db      DbApi
    Cloud   cloud.Cloud
}

func InitializeApp() App {
    wire.Build(ProvideDbApi, wire.Value(cloud.Azure{}), {{- range . }} Provide{{.ModelName}}Repository, {{ end -}})
    return App{}
}
